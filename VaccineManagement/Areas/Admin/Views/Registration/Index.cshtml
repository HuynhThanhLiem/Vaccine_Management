@{
    Layout = "_CRUDLayout";
}


<!--Main Content-->
<div id="fh5co-main">
    <div class="fh5co-narrow-content animate-box" data-animate-effect="fadeInLeft">
        <div class="fh5co-header">
            <h2 class="fh5co-heading animate-box" data-animate-effect="fadeInLeft">REGISTRATION</h2>
            <div class="fh5co-heading-search">
                <i class="fas fa-search"></i>
                <input class="fh5co-heading-search-put animate-box" type="text" placeholder="Search" data-animate-effect="fadeInLeft">
            </div>
        </div>
        <div class="create-table-all" data-animate-effect="fadeInLeft">
            <div class="create-vaccine" data-animate-effect="fadeInLeft">
                <a asp-action="UpSert" asp-controller="Registration" ><button class="fh5co-head-create-chuyen btn-green-hover">CREATE NEW</button></a>
            </div>
            <!--Table-->
            <div class="table-section">
                <table id="DT_load_reg" class="table table-striped table-bordered table-hover" width="100%" data-animate-effect="fadeInLeft">
                    <thead data-animate-effect="fadeInLeft">
                        <tr>
                            <th>Registration ID</th>
                            <th>Citizen ID</th>
                            <th>Anamnesis ID</th>
                            <th>Agreement</th>
                            <th>Choice Injections</th>
                            <th>Registrated Date</th>
                            <th>Status</th>
                            <th></th>
                        </tr>
                    </thead>
                </table>
            </div>
            <a asp-action="ExportToExcel" asp-controller="Registration">Export Excel</a>
            <!--End Table-->
        </div>
    </div>
</div>


    

